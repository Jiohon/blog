{
  "name": "blog",
  "title": "Johon Blog",
  "description": "Johon's personal website.",
  "author": "Johon",
  "private": true,
  "homepage": "https://blog.hushes.cn",
  "version": "1.1.9",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jiohon/blog.git"
  },
  "keywords": [
    "blog",
    "gatsby",
    "react",
    "typescript",
    "mdx",
    "personal-website"
  ],
  "bugs": {
    "url": "https://github.com/Jiohon/blog/issues"
  },
  "scripts": {
    "dev": "gatsby clean && gatsby develop -H 0.0.0.0 -p 8008 --verbose",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build --prefix-paths --verbose",
    "build:clean": "gatsby clean && gatsby build --prefix-paths --verbose",
    "serve": "gatsby serve -H 0.0.0.0 -o",
    "clean": "gatsby clean",
    "info": "gatsby info -C",
    "update-submodule": "git submodule sync && git submodule update --init --recursive",
    "prepare": "husky install",
    "commitlint": "commitlint --edit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "fix": "pnpm format && pnpm lint:fix",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0",
    "ci": "pnpm lint:check && pnpm format:check && pnpm type-check && pnpm build"
  },
  "dependencies": {
    "@ant-design/colors": "^7.2.0",
    "@ant-design/fast-color": "^2.0.6",
    "@ant-design/icons": "^5.6.1",
    "@codesandbox/sandpack-react": "^2.20.0",
    "@giscus/react": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@react-spring/rafz": "^9.7.5",
    "@react-spring/web": "^9.7.5",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@stackblitz/sdk": "^1.11.0",
    "ahooks": "^3.8.4",
    "antd": "^5.24.9",
    "antd-style": "^3.6.1",
    "chroma-js": "^3.1.2",
    "codesandbox": "^2.2.3",
    "color": "^4.2.3",
    "dayjs": "^1.11.13",
    "gatsby": "^5.14.1",
    "gatsby-plugin-feed": "^5.14.0",
    "gatsby-plugin-mdx": "^5.14.1",
    "gatsby-plugin-react-svg": "^3.3.0",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-remark": "^6.14.0",
    "glob": "^11.0.2",
    "graphql": "^16.11.0",
    "lodash": "^4.17.21",
    "prism-react-renderer": "^2.4.1",
    "prismjs": "^1.30.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reading-time": "^1.5.0",
    "rehype-katex": "3.1.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "1.0.0",
    "remark-math": "3.0.1",
    "three": "^0.175.0",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@types/node": "^20.17.32",
    "@types/prop-types": "^15.7.14",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.7",
    "@types/three": "^0.155.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^8.0.3",
    "node-polyfill-webpack-plugin": "^3.0.0",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "typescript": "~5.1.6",
    "unified": "10.1.2",
    "unist-util-visit": "2.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.0.0"
}
